{
	"name": "microsoft/microsoft-graph-core",
	"type": "library",
	"description": "The Microsoft Graph Core SDK for PHP",
	"homepage": "https://developer.microsoft.com/en-us/graph",
	"license": "MIT",
	"authors": [
		{
			"name": "Microsoft Graph Client Tooling",
			"email": "graphtooling@service.microsoft.com",
			"role": "Developer"
		}
	],
	"repositories": [
		{
			"type": "path",
			"url": "./kiota/abstractions/php",
			"only": ["microsoft/kiota-abstractions"],
			"options": {
				"symlink": false
			}
		},
		{
			"type": "path",
			"url": "./kiota/authentication/php/phpleague",
			"only": ["microsoft/kiota-authentication-phpleague"],
			"options": {
				"symlink": false
			}
		},
		{
			"type": "path",
			"url": "./kiota/http/php/guzzle",
			"only": ["microsoft/kiota-http-guzzle"],
			"options": {
				"symlink": false
			}
		},
		{
			"type": "path",
			"url": "./kiota/serialization/php/json",
			"only": ["microsoft/kiota-serialization-json"],
			"options": {
				"symlink": false
			}
		}
	],
	"require": {
		"php": "^8.0 || ^7.4",
		"microsoft/kiota-authentication-phpleague": "@dev",
		"microsoft/kiota-http-guzzle": "@dev",
		"microsoft/kiota-serialization-json": "@dev",
		"microsoft/kiota-abstractions": "@dev",
		"guzzlehttp/guzzle": "^7.0",
		"guzzlehttp/psr7": "^2.0",
		"php-http/httplug": "^2.2",
		"php-http/guzzle7-adapter": "^1.0",
		"ext-json": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.0",
		"mikey179/vfsstream": "^1.2",
		"phpstan/phpstan": "^0.12.90 || ^1.0.0"
	},
	"autoload": {
		"psr-4": {
			"Microsoft\\Graph\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Microsoft\\Graph\\Test\\": "tests/"
		}
	}
}
